{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"MongoDbSettings": {
		"ConnectionString": "mongodb://admin:4321@localhost:27000/Catalog?authSource=admin",
		"DatabaseName": "Catalog"
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft" : "Warning",
				"Microsoft.AspNetCore": "Warning",
				"System" : "Warning"
			}
		},
		"Enrich": [
			"FromLogContex"
		]
	}
}